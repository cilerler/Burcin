{
	"Logging": {
		"IncludeScopes": true,
		"LogLevel": {
			"Default": "Trace",
			"System": "Trace",
			"Microsoft": "Trace"
		},
		"Debug": {
			"LogLevel": {
				"Default": "Trace"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Trace"
			}
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Verbose"
		},
		"Using": [
			"Serilog.Exceptions",
			"Serilog.Sinks.Literate",
			"Serilog.Sinks.Seq"
		],
		"WriteTo": [
			{
				"Name": "LiterateConsole"
			},
			{
				"Name": "Seq",
				"Args": {
					"serverUrl": "http://localhost:5341"
				}
			}
		],
		"Properties": {
			"Configuration": "DEVELOPMENT"
		}
	}
}
