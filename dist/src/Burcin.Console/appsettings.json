{
  "ConnectionStrings": {
    "DefaultConnection": "data source=localhost;initial catalog=(databaseName);Trusted_Connection=True;MultipleActiveResultSets=True;App=Burcin"
  },
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Using": [
      "Serilog",
      "Serilog.Exceptions"
    ],
    "WriteTo": [],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Configuration": "DEFAULT",
      "ApplicationName": "Burcin"
    }
  },
  "Migration": {
    "AssemblyName": "Burcin.Migrations"
  },
  "BurcinHelper": {

  }
}