{
  "ConnectionStrings": {
    "SqlServerCacheConnection": "data source=localhost;initial catalog=DistributedCache;Trusted_Connection=True;MultipleActiveResultSets=true;App=Burcin.Api",
    "RedisCacheConnection": "localhost"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Using": [
      "Serilog",
      "Serilog.Exceptions"
    ],
    "WriteTo": [],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Configuration": "DEFAULT",
      "ApplicationName": "Burcin"
    }
  },
  "Migration": {
    "AssemblyName": "Burcin.Migrations"
  },
  "Helper": {
    "Prefix": "Burcin.Helper.Prefix.AppSettings.Json"
  },
  "Cache": {
    "SqlServer": {
      "ConnectionStringKey": "SqlServerCacheConnection",
      "SchemaName": "cache",
      "TableName": "BurcinApi"
    },
    "Redis": {
      "ConnectionStringKey": "RedisCacheConnection",
      "InstanceName": "BurcinApi:"
    }
  }
}