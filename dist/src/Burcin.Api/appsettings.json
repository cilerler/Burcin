{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "Using": [
      "Serilog",
      "Serilog.Exceptions"
    ],
    "WriteTo": [],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Configuration": "DEFAULT",
      "ApplicationName": "Burcin"
    }
  },
  "Migration": {
    "AssemblyName": "Burcin.Migrations"
  },
  "Helper": {
    "Prefix": "Burcin.Helper.Prefix.AppSettings.Json"
  }
}