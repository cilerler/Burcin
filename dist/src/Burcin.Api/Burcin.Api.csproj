<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>netcoreapp2.2</TargetFramework>
		<LangVersion>latest</LangVersion>
		<AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
		<DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
		<UserSecretsId>Burcin.Api</UserSecretsId>
		<!--#if (DockerSupport)-->
		<DockerComposeProjectPath>..\..\docker-compose.dcproj</DockerComposeProjectPath>
		<!--#endif-->
	</PropertyGroup>
	<!--#if (TemplateEditMode)-->
	<PropertyGroup>
		<DefineConstants>$(DefineConstants);BlazorApplication;EntityFramework;CacheSqlServer;CacheRedis;BackgroundService;HealthChecks;Swagger;ResponseCaching;</DefineConstants>
		<!-- Reference https://github.com/dotnet/templating/issues/1438 -->
		<BlazorApplication>true</BlazorApplication>
		<EntityFramework>true</EntityFramework>
		<CacheSqlServer>true</CacheSqlServer>
		<CacheRedis>true</CacheRedis>
		<BackgroundService>true</BackgroundService>
		<HealthChecks>true</HealthChecks>
		<Swagger>true</Swagger>
		<ResponseCaching>true</ResponseCaching>
	</PropertyGroup>
	<!--#endif-->
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<PlatformTarget>x64</PlatformTarget>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<PlatformTarget>x64</PlatformTarget>
		<Optimize>true</Optimize>
	</PropertyGroup>

	<PropertyGroup>
		<BuildNumber Condition=" '$(BuildNumber)' == '' ">0.0.0</BuildNumber>
		<VersionPrefix>$(BuildNumber)</VersionPrefix>
		<VersionSuffix Condition=" '$(Configuration)' == 'Debug' ">dev</VersionSuffix>
		<Product>Burcin.Api</Product>
		<Authors>(authors-xml-encoded)</Authors>
		<RepositoryUrl>(repositoryUrl-xml-encoded)</RepositoryUrl>
	</PropertyGroup>
	<ItemGroup>
		<Folder Include="wwwroot\" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="2.2.0" PrivateAssets="All" />
		<PackageReference Include="Microsoft.AspNetCore.App" />
		<!--#if (BlazorApplication)-->
		<PackageReference Include="Microsoft.AspNetCore.Blazor.Server" Version="0.7.0" />
		<!--#endif-->
		<PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.5.1" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="2.2.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="2.2.0" />
		<PackageReference Include="Microsoft.Extensions.Caching.SqlServer" Version="2.2.0" Condition="'$(CacheSqlServer)' == 'true'" />
		<PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="2.2.0" Condition="'$(CacheRedis)' == 'true'" />
		<PackageReference Include="Microsoft.AspNetCore.Diagnostics.HealthChecks" Version="2.2.0" Condition="'$(HealthChecks)' == 'true'" />
	</ItemGroup>
	<!--#if (HealthChecks)-->
	<ItemGroup>
		<PackageReference Include="AspNetCore.HealthChecks.UI" Version="2.2.5" />
		<PackageReference Include="AspNetCore.HealthChecks.System" Version="2.2.1" />
		<PackageReference Include="AspNetCore.HealthChecks.Network" Version="2.2.0" />
		<PackageReference Include="AspNetCore.HealthChecks.Uris" Version="2.2.1" />
		<PackageReference Include="AspNetCore.HealthChecks.SqlServer" Version="2.2.0" />
		<PackageReference Include="AspNetCore.HealthChecks.RabbitMQ" Version="2.2.0" />
		<PackageReference Include="AspNetCore.HealthChecks.Redis" Version="2.2.2" />
		<PackageReference Include="AspNetCore.HealthChecks.Elasticsearch" Version="2.2.0" />
		<PackageReference Include="AspNetcore.HealthChecks.Publisher.ApplicationInsights" Version="2.2.1" />
	</ItemGroup>
	<!--#endif-->
	<ItemGroup>
		<PackageReference Include="Serilog.Extensions.Logging" Version="2.0.2" />
		<PackageReference Include="Serilog.Settings.Configuration" Version="3.0.1" />
		<PackageReference Include="Serilog.Enrichers.Context" Version="4.0.0" />
		<PackageReference Include="Serilog.Enrichers.Environment" Version="2.1.2" />
		<PackageReference Include="Serilog.Enrichers.Process" Version="2.0.1" />
		<PackageReference Include="Serilog.Enrichers.Thread" Version="3.0.0" />
		<PackageReference Include="Serilog.Exceptions" Version="4.1.0" />
		<PackageReference Include="Serilog.Sinks.Console" Version="3.1.1" />
		<PackageReference Include="Serilog.Sinks.Seq" Version="4.0.0" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Ruya.Extensions.Logging" Version="1.0.485" />
		<PackageReference Include="Ruya.Helpers.Primitives" Version="1.0.486" />
		<PackageReference Include="Ruya.Primitives" Version="1.0.487" />
	</ItemGroup>
	<ItemGroup>
		<!--#if (BackgroundService)-->
		<PackageReference Include="ncrontab" Version="3.3.0" />
		<!--#endif-->
		<!--#if (Swagger)-->
		<PackageReference Include="Swashbuckle.AspNetCore" Version="4.0.1" Condition="'$(Swagger)' == 'true'" />
		<!--#endif-->
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\Burcin.Domain\Burcin.Domain.csproj" />
		<!--#if (BlazorApplication)-->
		<ProjectReference Include="..\Burcin.Web\Burcin.Web.csproj" />
		<!--#endif-->
	</ItemGroup>
	<ItemGroup>
	  <Content Update="appsettings.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Update="appsettings.Development.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Update="appsettings.Production.json">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>
	<Import Project="EntityFramework.targets" Condition="Exists('EntityFramework.targets')" />
</Project>
